!!
!! rxvt-unicode (urxvt)
!!

Xft.autohint:   0
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcddefault
URxvt*imLocale: en_US.UTF-8
URxvt*iso14755:	false

URxvt.geometry:             80x24
URxvt.scrollBar:  	    false
URxvt.cursorBlink:          true
URxvt.cursorColor:          #black
URxvt.cursorUnderline:      false
URxvt.saveLines:            2048
URxvt.lineSpace:            0
URxvt.letterSpace:  	    0

!! Font
URxvt.font:              xft:Cica:pixelsize=13:antialiase=true
URxvt.boldFont:          xft:Cica:bold:pixelsize=13:antialiase=true
URxvt.italicFont:        xft:Cica:italic:pixelsize=13:antialiase=true
URxvt.bolditalicFont:    xft:Cica:bold:italic:pixelsize=13:antialiase=true

!! Color Scheme
URxvt.color0:               #1C1D19
URxvt.color1:               #D01B24
URxvt.color2:               #A7D32C
URxvt.color3:               #D8CF67
URxvt.color4:               #61B8D0
URxvt.color5:               #695ABB
URxvt.color6:               #D53864
URxvt.color7:               #FEFFFE
URxvt.color8:               #1C1D19
URxvt.color9:               #D12A24
URxvt.color10:              #A7D32C
URxvt.color11:              #D8CF67
URxvt.color12:              #61B8D0
URxvt.color13:              #695ABB
URxvt.color14:              #D53864
URxvt.color15:              #FEFFFE
URxvt.foreground:           #F6F5EE
URxvt.background:           #1D1D19

! misc
URxvt.perl-ext-common:		default,clipboard,matcher
URxvt.keysym.C-C:		perl:clipboard:copy
URxvt.keysym.C-V:		perl:clipboard:paste
URxvt.url-launcher:		/usr/bin/chromium
URxvt.matcher.button:		1
URxvt.url-select.underline:	true


!!
!! xterm
!!

xterm*VT100.translations:  #override <Btn1Up>: select-end(PRIMARY, CLIPBOARD, CUT_BUFFER0)
XTerm*geometry:		   80x24
XTerm*termName:        	   xterm-256color
XTerm*utf8:		   1
XTerm*locale:		   true
XTerm*selectToClipboard:   true
XTerm*cjkWidth:		   true
XTerm*saveLines:	   2048
XTerm*lineSpace:           0
XTerm*letter_space:	   0
xterm*faceName: 	   Cica:size=12:style=Regular:antialias=true
xterm*faceNameDoublesize:  Cica:size=12:style=Regular:antialias=true
xterm*faceSize: 	   14
xterm*borderWidth: 	   0

